echo 30 1/32 4   1 0.35 1000   examples/vdp-1.coeff  1 1    1 1 1 0 | time ./PIVP -d |& tee vdp1a.erg
grep "^bs" vdp1a.erg | cut -d'c' -f2 | cut -d')' -f1 | sed -e "s/\*2^(/ /"  > vdp1a.bs
gnuplot> 
plot  "../PIVP-08/vdp1.bs" u 0:(log($1)/log(2)+$2)  w l, "vdp1.bs" u 0:(log($1)/log(2)+$2)  w l, "vdp1b.bs" u 0:(log($1)/log(2)+$2)  w l



 grep  "^bs"  vdp3-1000.res  | cut -d'c' -f2 | cut -d')' -f1 | sed -e "s/\*2^(/ /"  > vdp3-1000.bs
gnuplot plot  "vdp3-1000.bs" u 0:(log($1)/log(2)+$2)  w l




export VER=o
echo 25 6/32 4   1 0.35 5000   examples/vdp-3.coeff  1 1    1 1 1 0 | time ./PIVP -d --prec_start=3 |& tee ./vdp3-5000${VER}.res
grep  "^bs"  vdp3-5000${VER}.res  | cut -d'c' -f2 | cut -d')' -f1 | sed -e "s/\*2^(/ /"  > vdp3-5000${VER}.bs












gnuplot> 
 plot [0:15000][-200:-60]  "vdp3-5000.bs" u 0:(log($1)/log(2)+$2)  w l not,   "vdp3-5000a.bs" u 0:(log($1)/log(2)+$2)  w l not,  "vdp3-5000b.bs" u 0:(log($1)/log(2)+$2)  w l not,   "vdp3-5000c.bs" u 0:(log($1)/log(2)+$2)  w l not,  "vdp3-5000d.bs" u 0:(log($1)/log(2)+$2)  w l not, "vdp3-5000e.bs" u 0:(log($1)/log(2)+$2) ls 2  w l not

















